{
  "name": "api",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "@types/cors": "^2.8.12",
    "axios": "^0.27.2",
    "babel-preset-env": "^1.7.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "jest": "^28.1.3",
    "superagent": "^8.0.0",
    "supertest": "^6.2.4",
    "tsconfig-paths": "^4.1.0"
  },
  "include": [
    "src/**/*"
  ],
  "transform": {
    "\\.js$": "<rootDir>/node_modules/babel-jest"
  },
  "scripts": {
    "dev": "ts-node-dev -r tsconfig-paths/register --inspect --transpile-only --ignore-watch node_modules src/server.ts",
    "start": "ts-node -r tsconfig-paths/register src/server.ts",
    "typeorm": "ts-node-dev -r tsconfig-paths/register -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "test": "jest --verbose --reporters default --detectOpenHandles --runInBand --testTimeout=60000"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.18.6",
    "@types/axios": "^0.14.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.6",
    "@types/supertest": "^2.0.12",
    "ts-jest": "^28.0.7",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.4"
  }
}
